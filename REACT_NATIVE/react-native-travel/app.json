{
  "expo": {
    "name": "react-native-travel",
    "slug": "react-native-travel",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "react-native-travel",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
    "eas": {
      "projectId": "your-project-id"
    }
    },
    "oauth": {
      "redirectUri": "https://auth.expo.io/@ka82/react-native-travel"
    }
  }
}
